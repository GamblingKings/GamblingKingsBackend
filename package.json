{
  "name": "GamblingKingsBackend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/yummyblabla/GamblingKingsBackend.git",
  "author": "GamblingKings",
  "license": "MIT",
  "scripts": {
    "dev:cleanup": "rm -rf .build/ .serverless/ .dynamodb/ .webpack/",
    "dev:run": "sls dynamodb install && sls offline start",
    "cleanup_tables": "cd scripts/ && chmod 777 dev_cleanup_tables.sh && ./dev_cleanup_tables.sh",
    "start_local": "npm-run-all format install_dep dev:*",
    "install_dep": "yarn install",
    "deploy": "aws-vault exec gamblingkings-sls --no-session -- serverless deploy",
    "remove": "aws-vault exec gamblingkings-sls --no-session -- serverless remove",
    "start": "npm-run-all format dev:cleanup install_dep deploy",
    "format": "prettier --write \"**/*.{js,ts,md,json,yml}\"",
    "lint": "eslint -c .eslintrc.json . --ext .js,.ts --fix",
    "test": "jest && codecov"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.59",
    "@types/node": "^14.0.24",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.5",
    "@types/lambda-tester": "^3.6.0",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.7.0",
    "@typescript-eslint/parser": "^3.7.0",
    "aws-sdk": "^2.718.0",
    "aws-sdk-mock": "^5.1.0",
    "eslint": "7.5.0",
    "eslint-config-airbnb-base": "14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.1.0",
    "jest-dynalite": "^3.0.0",
    "jest-extended": "^0.11.5",
    "lambda-tester": "^4.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "^6.5.0",
    "serverless-webpack": "^5.3.3",
    "ts-jest": "^26.1.3",
    "ts-loader": "^8.0.1",
    "typescript": "^3.9.7",
    "webpack": "^4.43.0",
    "webpack-node-externals": "^2.5.0",
    "wscat": "^4.0.1"
  }
}
