{
  "name": "GamblingKingsBackend",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/yummyblabla/GamblingKingsBackend.git",
  "author": "Vincent Wong <vwong125@my.bcit.ca>",
  "license": "MIT",
  "scripts": {
    "dev:cleanup": "rm -rf .build/ .serverless/ .dynamodb/ .webpack/",
    "dev:run": "sls dynamodb install && sls offline start",
    "cleanup_tables": "cd scripts/ && chmod 777 dev_cleanup_tables.sh && ./dev_cleanup_tables.sh",
    "start_local": "npm-run-all format install_dep dev:*",
    "install_dep": "yarn install",
    "deploy": "aws-vault exec gamblingkings-sls --no-session -- serverless deploy",
    "remove": "aws-vault exec gamblingkings-sls --no-session -- serverless remove",
    "start": "npm-run-all format dev:cleanup install_dep deploy",
    "format": "prettier --write \"**/*.{js,ts,md,json,yml}\"",
    "lint": "eslint -c .eslintrc.json . --ext .js,.ts --fix",
    "test": "jest"
  },
  "dependencies": {
    "@types/aws-lambda": "^8.10.56",
    "@types/node": "^14.0.13",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.0",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "aws-sdk": "^2.696.0",
    "aws-sdk-mock": "^5.1.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prettier": "^3.1.3",
    "jest": "^26.0.1",
    "jest-dynalite": "^1.1.9",
    "jest-extended": "^0.11.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "serverless-dynamodb-local": "^0.2.39",
    "serverless-offline": "^6.4.0",
    "serverless-webpack": "^5.3.2",
    "ts-jest": "^26.1.0",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.5",
    "webpack": "^4.43.0",
    "webpack-node-externals": "^1.7.2",
    "wscat": "^4.0.1"
  }
}
